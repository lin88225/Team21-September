final int SCREENX = 550;
final int SCREENY = 367;
final float width = 500;
final float heigth = 500;

PImage bg;

void setup()
{
  //size(550,368);
  size(550, 500);
  background(255);
  bg = loadImage("usa-map.jpg");

}
void draw()
{
    image(bg, 0, 0);
    image(bg, SCREENX, SCREENY, width, height);
}
